    <div class="menu">
        <div class="container">
            <ul class="menu__ul">
                <li class="menu__ul-elem selected" title="бакалавриат" id="bachelor">common questions from IU alumni</li>
                <li class="menu__ul-elem" title="магистратура" id="magistracy"> </li>
                <li class="menu__ul-elem" title="аспирантура" id="postgraduate"> </li>
                <li class="menu__ul-elem" title="грант" id="grant"> </li>
            </ul>
