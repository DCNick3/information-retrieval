 ﻿<script>
function change_rate(){

   var rate=jQuery("#rate").val();
   jQuery("#currencyContainer").html('<div style="text-align:center;width:100%;"><img width="100px" id="loading" src="https://alfajr-news.net/template/loading.gif"></div>');
   jQuery.post("https://alfajr-news.net/template/currencyservice.php?rate="+rate,{rate_value:rate},function(result){
    jQuery("#currencyContainer").html(result);
  });
  
  
}
jQuery(function() {
jQuery("#rate").val('AED');
change_rate();
}
)
</script>