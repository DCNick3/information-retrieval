<script>
	$(document).ready(function(){
		
		$('#example1').ideaboxWeather({
			location		:'Abu Dhabi,UAE',
			lang			:'ar',
		
		
		});
		
		/*this code only demo page big view for codes*/
		
		$(".i-right a, .description a").on("click",function(event){
			event.preventDefault();
			$("body").append("<div class='ipopup' onclick='closepopup();'><h2 onclick='closepopup();'>CLOSE</h2><img src='"+$(this).attr("href")+"'></div>");
		});
				
		/*this code only demo page big view for codes*/
	});	
	
	function closepopup()
	{
		$(".ipopup").remove();
	}
</script> 
  
 

